{
  "name": "webvisual-server",
  "version": "0.8.0",
  "description": "server for visualization of data changes",
  "private": "true",
  "homepage": "https://github.com/fooloomanzoo/webvisual",
  "author": "Johannes Brautzsch <hannes-b@web.de>",
