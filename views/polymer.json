{
  "entrypoint": "index.html",
  "shell": "src/webvisual-app.html",
  "fragments": [
    "src/page-home.html",
    "src/page-list.html",
    "src/page-groups.html",
    "src/page-detail.html",
    "src/lazy-resources.html"
  ],
  "sources": [
    "src/**/*",
    "scripts/**/*",
    "polyfills/**/*",
    "fonts/**/*",
    "icons/**/*",
    "data/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*",
    "manifest.json",
    "locales.json",
    "app.yaml"
  ],
  "builds": [ {
    "name": "compiled",
    "bundle": true,
    "js": {
      "minify": true,
      "compile": true
    },
    "css": {
      "minify": true
    },
    "html": {
      "minify": true
    },
    "addServiceWorker": true
  } ],
  "lint": {
    "rules": [ "polymer-2-hybrid" ]
  }
}
